{
  "extends": "./tsconfig.react.json",
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "incremental": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  }
}
